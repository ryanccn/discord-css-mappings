{
	"compilerOptions": {
		"strict": true,
		"downlevelIteration": true,
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "node",
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules", "**/node_modules/*"]
}
